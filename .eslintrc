{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "warn",
      { "extensions": [".js", ".jsx", ".tsx", ".ts"] }
    ],
    "import/extensions": [
      0,
      {
        "ts": "never",
        "tsx": "never"
      }
    ]
  }
}
